package javaapplication19;


public class JavaApplication19 {

    
    public static void main(String[] args) {
        int[] dizi={1,7,6,4,5};
        int sayac=0;
     
       for(int i=0; i<dizi.length; i++){
           for(int j=i+1; j<dizi.length; j++)
       if(dizi[i]>dizi[j]){
       sayac=sayac+1;
       }
       }
       System.out.println("evrilen sayÄ±"+sayac);
    }
    
}
